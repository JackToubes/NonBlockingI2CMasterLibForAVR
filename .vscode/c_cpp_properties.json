{
    "configurations": [
        {
            "name": "AVR",
            "intelliSenseMode": "${default}",
            "compilerPath": "C://Winavr/bin/avr-gcc.exe",
            "cStandard": "${default}",
            "cppStandard": "${default}",
            "includePath": [
                "C:\\WinAVR\\avr\\include\\**",
                "C:\\WinAVR\\avr\\include\\avr\\**"
            ],
            "compilerArgs": [
                "-g",
                "-Os",
                "-Wall",
                "-Wextra",
                "-fpermissive",
                "-fno-exceptions",
                "-fno-threadsafe-statics",
                "-pipe"
            ]
        }
    ],
    "version": 4
}